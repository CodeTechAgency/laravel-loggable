{
  "name": "codetech/laravel-loggable",
  "description": "Laravel package for handling publishable resources.",
  "keywords": [
    "codetech",
    "laravel-loggable",
    "laravel",
    "crud",
    "logging"
  ],
  "authors": [
    {
      "name": "José Osório",
      "email": "jfrosorio@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "laravel/framework": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "CodeTech\\Loggable\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
